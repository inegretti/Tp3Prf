<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout10"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments.AgregarEjNewFragment" >

    <TextView
        android:id="@+id/textView4"
        android:layout_width="164dp"
        android:layout_height="39dp"
        android:layout_marginStart="155dp"
        android:layout_marginTop="69dp"
        android:layout_marginEnd="155dp"
        android:layout_marginBottom="34dp"
        android:text="Ingreso de nuevo ejercicio"
        app:layout_constraintBottom_toTopOf="@+id/ingNewEjNom"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnIngNewEj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="147dp"
        android:layout_marginEnd="155dp"
        android:layout_marginBottom="103dp"
        android:text="Ingresar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextTextMultiLine2"
        android:layout_width="260dp"
        android:layout_height="166dp"
        android:layout_marginStart="75dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="76dp"
        android:layout_marginBottom="48dp"
        android:ems="10"
        android:gravity="start|top"
        android:hint="comentario"
        android:inputType="textMultiLine"
        app:layout_constraintBottom_toTopOf="@+id/btnIngNewEj"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ingNewEjMul" />

    <EditText
        android:id="@+id/ingNewEjNom"
        android:layout_width="281dp"
        android:layout_height="64dp"
        android:layout_marginStart="57dp"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="79dp"
        android:layout_marginBottom="58dp"
        android:ems="10"
        android:hint="Ingrese nombre del ejercicio"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/ingNewEjMul"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <EditText
        android:id="@+id/ingNewEjMul"
        android:layout_width="275dp"
        android:layout_height="48dp"
        android:layout_marginStart="57dp"
        android:layout_marginTop="58dp"
        android:layout_marginEnd="79dp"
        android:layout_marginBottom="20dp"
        android:ems="10"
        android:hint="ingrese link de multimedia"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextMultiLine2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ingNewEjNom" />

</androidx.constraintlayout.widget.ConstraintLayout>