<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments.AdminFragment">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/adminView"
        android:layout_width="350dp"
        android:layout_height="388dp"
        android:layout_marginStart="45dp"
        android:layout_marginEnd="47dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="136dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textAdm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="176dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="177dp"
        android:layout_marginBottom="4dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/adminView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.71" />

    <Button
        android:id="@+id/btnIngUs"
        android:layout_width="143dp"
        android:layout_height="56dp"
        android:layout_marginStart="32dp"
        android:text="Alta de Usuarios"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="549dp" />

    <Button
        android:id="@+id/btnIANE"
        android:layout_width="148dp"
        android:layout_height="58dp"
        android:layout_marginEnd="28dp"
        android:text="Alta Nuevo Ejercicio"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="549dp" />

    <Button
        android:id="@+id/btnLgAd"
        android:layout_width="157dp"
        android:layout_height="65dp"
        android:layout_marginStart="75dp"
        android:layout_marginTop="95dp"
        android:layout_marginEnd="75dp"
        android:layout_marginBottom="48dp"
        android:text="Cerrar Sesion"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/adminView" />
</androidx.constraintlayout.widget.ConstraintLayout>